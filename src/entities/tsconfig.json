{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ES2022",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "removeComments": true,
    "strictNullChecks": true,
    "noImplicitAny": true
  }
}
