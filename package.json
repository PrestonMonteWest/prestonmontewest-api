{
  "name": "api",
  "version": "1.1.6",
  "private": true,
  "project": "prestonmontewest",
  "scripts": {
    "start": "PORT=3000 node index.js",
    "nodemon": "PORT=3000 nodemon index.js",
    "start:docker": "docker run --env-file .env -p 3000:3000 $npm_package_project/$npm_package_name:$npm_package_version",
    "push": "docker push $npm_package_project/$npm_package_name:$npm_package_version",
    "build": "docker build --build-arg port=3000 -t $npm_package_project/$npm_package_name:$npm_package_version .",
    "postbuild": "npm run push"
  },
  "dependencies": {
    "aws-sdk": "^2.579.0",
    "express": "~4.16.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.1"
  }
}
